

<div class="layout">
    <h1>Please log in.</h1>

    <form method="post">
        <label for="email">Email Address</label>
        <input type="text" name="email" />
        <label for="password">Password</label>
        <input type="password" name="password" />
        <button type="submit">LOG IN</button>
    </form>
    {{> error}}
</div>